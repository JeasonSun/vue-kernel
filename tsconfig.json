{
  "compilerOptions": {
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "target": "esnext",
    "module": "esnext",
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "downlevelIteration": true,
    "lib": [
      "es6",
      "DOM"
    ],
    "baseUrl": ".",
    "paths": {
      "@vue-kernel/*": [
        "packages/*/src"
      ]
    }
  },
  // "include": ["src/index.ts", "src/global.d.ts"]
}